<UserControl x:Class="HydroModTools.Client.Wpf.Controls.CreateModsTabControl"
             x:ClassModifier="internal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HydroModTools.Client.Wpf.Controls"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="1005">
    <hc:TabControl x:Name="TabSelector" SelectedIndex="0" ShowOverflowButton="False" ShowContextMenu="False">
        <hc:TabItem x:Name="ProjectsTab" Header="Projects" FontFamily="Consolas" IsSelected="True" />
        <hc:TabItem x:Name="GUIDsTab" Header="GUIDs" FontFamily="Consolas" />
        <hc:TabItem x:Name="UIDsTab" Header="UIDs" FontFamily="Consolas" />
    </hc:TabControl>
</UserControl>
