<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:HydroToolChain.Avalonia.ViewModels.Sections"
             x:DataType="vm:ProjectsHeader"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="HydroToolChain.Avalonia.Views.Sections.ProjectsHeader">
    <Design.DataContext>
        <vm:ProjectsHeader/>
    </Design.DataContext>
    <StackPanel
        Orientation="Horizontal"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Top">
        <DockPanel
            Dock="Left"
            HorizontalAlignment="Stretch">
            <Button />
        </DockPanel>
        <DockPanel
            HorizontalAlignment="Stretch">
            <Button />
            <Button />
            <Button />
            <Button />
        </DockPanel>
        <DockPanel
            Dock="Right"
            HorizontalAlignment="Stretch"> 
            <Button />
            <Button />
        </DockPanel>
    </StackPanel>
</UserControl>
